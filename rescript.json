{
  "name": "react-trace-front",
  "version": "0.1.0",
  "sources": [
    { "dir": "app", "subdirs": true },
    { "dir": "bindings", "subdirs": true },
    { "dir": "components", "subdirs": true },
    { "dir": "shared", "subdirs": true }
